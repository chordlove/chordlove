{
  "tags": {
    "allowUnknownTags": true
  },
 "source": {
    "include": ["modules"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": false
  },
  "jsVersion": 180,
  "opts": {
    "recurse": true,
    "private": true,
    "encoding": "utf8",
    "destination": "./doc/"
  }
}
